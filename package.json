{
    "private": false,
    "type": "module",
    "license": "ISC",
    "version": "1.0.0",
    "name": "@termehui/validators",
    "description": "Extra validators for Yup",
    "main": "./dist/validators.umd.js",
    "module": "./dist/validators.es.js",
    "types": "./dist/validators.d.ts",
    "exports": {
        ".": {
            "import": "./dist/validators.es.js",
            "require": "./dist/validators.umd.cjs"
        }
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "yup",
        "validation",
        "validator"
    ],
    "authors": [
        {
            "email": "m.ajamekramy@gmail.com",
            "homepage": "http://ekramy.ir",
            "name": "Mojtaba Ekramy",
            "role": "Developer"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/termehui/validators"
    },
    "scripts": {
        "build:types": "tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --outDir dist",
        "build": "vite build && npm run build:types",
        "prepare": "npm run build",
        "test": "vitest"
    },
    "dependencies": {
        "@termehui/date-utils": "^1.0.2",
        "yup": "^1.2.0"
    },
    "devDependencies": {
        "@types/node": "^20.6.1",
        "typescript": "^5.2.2",
        "vite": "^4.4.9",
        "vitest": "^0.34.4"
    }
}
